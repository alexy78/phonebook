<h1>Edit Contact</h1>

<%= form_with(model: @contact, local: true) do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :phone %>
  <%= form.text_field :phone %>

  <%= form.label :email %>
  <%= form.text_field :email %>

  <%= form.submit %>
<% end %>

<%= link_to 'Back', contacts_path %>